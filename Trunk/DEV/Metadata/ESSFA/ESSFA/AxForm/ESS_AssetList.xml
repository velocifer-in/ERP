					<Method>
						<Name>initValue</Name>
						<Source><![CDATA[
        /// <summary>
        /// Generates new addition number for the selected fixed asset and sets its value.
        /// </summary>
        /// <remarks>
        /// The super method calls the initValue method of the data source table.
        /// </remarks>
        public void initValue()
        {
            
            super();
            AssetList_ES.ListId = AssetList_ES.getNextListId();
            AssetList_ES.Name = AssetTable::find(AssetList_ES.AssetId).Name + subStr(AssetList_ES.ListId,strLen(AssetList_ES.ListId)-3,4);
            //AssetMgmt_Prem
            AssetList_ES.AcquisitionDate = AssetBookTbl_Extension::findByAssetId(AssetList_ES.AssetId).AcquisitionDate;
            AssetList_ES.DefaultLocation = AssetParameters::find().AssetLocId_ES;
            AssetList_ES.LocType  = AssetLocType_ES::Location;
            if(AssetBookTbl_Extension::findByAssetId(AssetList_ES.AssetId).DefaultDimension &&
       AssetLocation_ES::find(AssetList_ES.DefaultLocation, false).DimensionDefault)
        AssetList_ES.DefaultDimension = AssetList_ES::mergeDimension(AssetBookTbl_Extension::findByAssetId(AssetList_ES.AssetId).DefaultDimension,
                                                               AssetLocation_ES::find(AssetList_ES.DefaultLocation, false).DimensionDefault);

            if(!AssetBookTbl_Extension::findByAssetId(AssetList_ES.AssetId).DefaultDimension &&
       AssetLocation_ES::find(AssetList_ES.DefaultLocation, false).DimensionDefault)
        AssetList_ES.DefaultDimension = AssetLocation_ES::find(AssetList_ES.DefaultLocation, false).DimensionDefault;

            if(AssetBookTbl_Extension::findByAssetId(AssetList_ES.AssetId).DefaultDimension &&
       !AssetLocation_ES::find(AssetList_ES.DefaultLocation, false).DimensionDefault)
        AssetList_ES.DefaultDimension = AssetBookTbl_Extension::findByAssetId(AssetList_ES.AssetId).DefaultDimension;
            //
        }

]]></Source>
