					<Method>
						<Name>remainingQty</Name>
						<Source><![CDATA[
        display AssetAdditionQty remainingQty()
        {
            return this.totalAssetQty() - this.totalListQty();
        }

]]></Source>
					</Method>
					<Method>
						<Name>totalAssetQty</Name>
						<Source><![CDATA[
        display AssetAdditionQty totalAssetQty()
        {
            AssetTable   _AssetTable;

            select _AssetTable
        where _AssetTable.AssetId == AssetList_ES.AssetId;

            return _AssetTable.Quantity;
        }

]]></Source>
					</Method>
					<Method>
						<Name>totalListQty</Name>
						<Source><![CDATA[
        display AssetAdditionQty totalListQty()
        {
            AssetList_ES           assetListValue;
            AssetAdditionQty    totalValue;

            while select AssetId, Quantity, UnitCost from assetListValue
          where assetListValue.AssetId == AssetList_ES.AssetId &&
                assetListValue.Status != AssetStatus_ES::Closed &&
                assetListValue.Stopped == NoYes::No
            {
                totalValue += assetListValue.Quantity;
            }
            return totalValue;
        }

]]></Source>
					</Method>
					<Method>
						<Name>validateWrite</Name>
						<Source><![CDATA[
        public boolean validateWrite()
        {
            AssetList_ES   _AssetList;
            AssetTable  _AssetTable = AssetTable::find(AssetList_ES.AssetId);
            boolean     ret;

            ret = super();

            select sum(Quantity) from _AssetList
        where _AssetList.AssetId == AssetList_ES.AssetId
            && _AssetList.RecId    != AssetList_ES.RecId;

            if((_AssetList.Quantity + AssetList_ES.Quantity) > _AssetTable.Quantity)
        ret = ret && checkFailed(strFmt("Total Asset List Quantity Should not exceed Asset Quantity : %1", _AssetTable.Quantity));


            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;

            ret = super();
            if(AssetList_ES.LocId != AssetParameters::find().AssetLocId_ES || AssetList_ES.MovementRef != '')
        //commented by din
          ///      AssetMove.enabled(false);//AssetMgmt_Prem
         //  else
       // AssetMove.enabled(true);
            //commented by din
            /*EditRecordButton.enabled((AssetList.DefaultLocation == AssetTransferJourPost::currentAssetLocation(AssetList.AssetListId)) ?
                              true : false);*/
            if(AssetList_ES.Stopped || AssetParameters::find().AssetLocId_ES != AssetList_ES.LocId)
        AssetList_ES_ds.allowEdit(false);
            else
        AssetList_ES_ds.allowEdit(true);

            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>remainingBalance</Name>
						<Source><![CDATA[
        display AmountMST remainingBalance()
        {
            return this.totalAcquisitionValue() - this.totalAssetValue();
        }

]]></Source>
					</Method>
					<Method>
						<Name>totalAssetValue</Name>
						<Source><![CDATA[
        display AmountMST totalAssetValue()
        {
            AssetList_ES       assetListValue;
            AmountMST       totalValue;

            while select AssetId, Quantity, UnitCost from assetListValue
          where assetListValue.AssetId == AssetList_ES.AssetId &&
                assetListValue.Status != AssetStatus_ES::Closed &&
                assetListValue.Stopped == NoYes::No
            {
                totalValue += assetListValue.Quantity * assetListValue.UnitCost;
            }
            return totalValue;
        }

]]></Source>
					</Method>
					<Method>
						<Name>totalAcquisitionValue</Name>
						<Source><![CDATA[
        // BP deviation documented
        display AmountMST totalAcquisitionValue()
        {
            AssetTrans      assetTransValue;
            AssetBook       assetBook;
            AssetBookTable  assetBookTable;

            select AssetId, BookId from assetBook
                where assetBook.AssetId == AssetList_ES.AssetId
           join BookId, CurrentOperationsTax from assetBookTable
                where assetBookTable.BookId == assetBook.BookId &&
                      assetBookTable.CurrentOperationsTax == CurrentOperationsTax::Current
            join sum(AmountMST) from assetTransValue
                where assetTransValue.AssetId == AssetList_ES.AssetId &&
                      assetTransValue.BookId  == assetBookTable.BookId &&
                      (assetTransValue.TransType == AssetTransType::Acquisition ||
                      assetTransValue.TransType == assetTransType::AcquisitionAdj);

            return assetTransValue.AmountMST;
        }

]]></Source>
