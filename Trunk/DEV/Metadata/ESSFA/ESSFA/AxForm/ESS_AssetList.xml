			<Method>
				<Name>UpdateDelJournalId</Name>
				<Source><![CDATA[
    private void UpdateDelJournalId()
    {
        AssetList_ES AssetListloc;
        LedgerJournalTable LedgerJournalTable;
        ttsBegin;
        update_recordSet AssetListloc setting MovementRef = ''
    notExists join LedgerJournalTable where LedgerJournalTable.JournalNum == AssetListloc.MovementRef;
        ttsCommit;
    }

]]></Source>
