			<Method>
				<Name>populateAssetLists</Name>
				<Source><![CDATA[
    void populateAssetLists()
    {
        Dialog                  dialog = new Dialog("Asset lists creation");
        DialogField             dialogQty, dialogValue;
        int                     listQty, assetPrice, i;

        AssetList_ES               assetListIns;

        dialogQty   = dialog.addFieldValue(extendedTypeStr(PurchQty), listQty,   'Quantity');
        dialogValue = dialog.addFieldValue(extendedTypeStr(Price),    assetPrice,'Asset value');
        if (dialog.run())
        {
            listQty    = dialogQty.value();
            assetPrice = dialogValue.value();
        }

        if (listQty != 0)
        {
            assetListIns.createAssetList(assetTableSelect, listQty, assetPrice);
        }
    }

]]></Source>
